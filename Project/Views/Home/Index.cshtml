
@{
        ViewBag.Title = "Index";
        Layout = "~/Views/Shared/_LayoutPage3.cshtml";
    }

<div class="row justify-content-md-start">
    <div class="col-md-3">
        <div class="card">
            <div class="card-header">
                Welcome @Session["FullName"].ToString()
            </div>
            <div class="card-body">
                @if (Session["FullName"] != null)
                {
                    @*<p>Name: @Session["FullName"].ToString()</p>*@
                    <p>Your Email ID : @Session["Email"].ToString()</p>
                   
                }
            </div>
        </div>
    </div>
</div>
